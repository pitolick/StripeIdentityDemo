{
  "name": "stripe-identity-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p next:dev storybook proxy",
    "next:dev": "next dev -p 3001",
    "proxy": "local-ssl-proxy --source 3000 --target 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:secretlint": "secretlint --secretlintignore .gitignore \"**/*\" --locale ja",
    "format": "prettier --write --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json,css}\" && npm run lint:secretlint",
    "format:check": "eslint-config-prettier \"./**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --coverage false",
    "test:coverage": "jest",
    "test:update-snapshot": "jest --update-snapshot",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@stripe/react-stripe-js": "^2.7.1",
    "@stripe/stripe-js": "^3.4.1",
    "@tailwindcss/typography": "^0.5.14",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "deepmerge": "^4.3.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.5",
    "next": "^14.2.12",
    "postcss": "8.4.41",
    "preline": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "stripe": "^16.7.0",
    "tailwindcss": "3.4.10",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@secretlint/secretlint-rule-preset-recommend": "^8.2.4",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-mdx-gfm": "^8.2.8",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.2.9",
    "@storybook/icons": "^1.2.10",
    "@storybook/nextjs": "^8.2.8",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/jest": "^29.5.12",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "local-ssl-proxy": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "secretlint": "^8.2.4",
    "storybook": "^8.2.8",
    "vercel": "^35.2.4"
  },
  "volta": {
    "node": "20.16.0"
  },
  "lint-staged": {
    "*": [
      "secretlint"
    ]
  }
}
